<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:m="urn:import:gwt.material.design.client.ui">
  <ui:with field="text" type="java.lang.String"/>
  <ui:with field="css" type="java.lang.String"/>
  <ui:with field="iconType" type="gwt.material.design.client.constants.IconType"/>
  <ui:style field="actionStyles" type="scoutmgr.client.view.cell.ActionCell.ActionStyle">
    .action {
      font-size: 12px;
    }

    .view {
      color: #3a7b4d;
    }

    .edit {
      color: #1a4f7b;
    }

    .delete {
      color: #d9534f
    }
  </ui:style>
  <g:HTMLPanel>
    <ui:text from="{text}"/>
    <m:MaterialButton text="foo" backgroundColor="white" textColor="red" waves="DEFAULT" />
  </g:HTMLPanel>
</ui:UiBinder>