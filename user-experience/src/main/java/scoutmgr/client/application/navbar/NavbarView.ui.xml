<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with type="scoutmgr.client.place.NameTokens" field="_nameTokens"></ui:with>
  <g:HTMLPanel>
    <m:MaterialNavBar ui:field="navBar" backgroundColor="blue" type="SHRINK">
      <m:MaterialNavBrand float="LEFT" text="Scout Manager"/>
      <m:MaterialNavSection float="RIGHT">
        <m:MaterialTooltip text="Account Settings" position="BOTTOM">
          <m:MaterialLink iconType="ACCOUNT_CIRCLE" iconPosition="NONE"/>
        </m:MaterialTooltip>
        <m:MaterialTooltip text="Apps" position="BOTTOM">
          <m:MaterialLink iconType="VIEW_MODULE" iconPosition="NONE"/>
        </m:MaterialTooltip>
        <m:MaterialTooltip text="Notifications" position="BOTTOM">
          <m:MaterialLink activates="sideNav" iconType="NOTIFICATIONS" iconPosition="NONE"/>
        </m:MaterialTooltip>
        <m:MaterialTooltip text="Search" position="BOTTOM">
          <m:MaterialLink ui:field="btnSearch" iconType="SEARCH" iconPosition="NONE"/>
        </m:MaterialTooltip>
      </m:MaterialNavSection>
    </m:MaterialNavBar>
  </g:HTMLPanel>
</ui:UiBinder>