<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with type="scoutmgr.client.place.NameTokens" field="_nameTokens"></ui:with>
  <ui:with field='_bundle' type='scoutmgr.client.resource.ScoutmgrResourceBundle'/>
  <ui:style gss="true">
    a {
      cursor: pointer;
    }
  </ui:style>
  <g:HTMLPanel>
    <nav class="{_bundle.bootstrap.navbar} {_bundle.bootstrap.navbarDefault} {_bundle.bootstrap.navbarFixedTop}">
      <div class="{_bundle.bootstrap.containerFluid}">
        <div class="{_bundle.bootstrap.navbarHeader}">
          <g:HTML addStyleNames="{_bundle.bootstrap.navbarToggle}" ui:field="_navToggle">
            <span class="{_bundle.bootstrap.iconBar}"></span>
            <span class="{_bundle.bootstrap.iconBar}"></span>
            <span class="{_bundle.bootstrap.iconBar}"></span>
          </g:HTML>
          <a class="{_bundle.bootstrap.navbarBrand}" href="#">Scout Manager</a>
        </div>
        <div ui:field="_navCollapse" class="{_bundle.bootstrap.navbarCollapse} {_bundle.bootstrap.collapse}">
          <ul class="{_bundle.bootstrap.nav} {_bundle.bootstrap.navbarNav}">
            <li ui:field="_eventsLinkContainer"><g:Anchor text="Events" href="#{_nameTokens.getEvents}"/></li>
            <li ui:field="_membersLinkContainer"><g:Anchor text="Scouts" href="#{_nameTokens.getScoutList}"/></li>
          </ul>
        </div>
      </div>
    </nav>
  </g:HTMLPanel>
</ui:UiBinder>