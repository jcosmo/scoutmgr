<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c='urn:import:com.google.gwt.user.cellview.client'
             xmlns:m="urn:import:gwt.material.design.client.ui">
  <ui:with field='_bundle' type='scoutmgr.client.resource.ScoutmgrResourceBundle'/>

  <g:HTMLPanel>
    <m:MaterialTitle title="Registered Members">
    </m:MaterialTitle>
    <m:MaterialRow>
      <c:DataGrid ui:field="_memberTable"/>
      <m:MaterialButton ui:field="_addScoutButton" type="FLOATING" text="Add Scout" backgroundColor="blue" iconType="ADD" size="LARGE"/>
    </m:MaterialRow>
  </g:HTMLPanel>
</ui:UiBinder>