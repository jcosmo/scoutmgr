             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field='_bundle' type='scoutmgr.client.resource.ScoutmgrResourceBundle'/>
  <g:HTMLPanel>
    <g:SimplePanel ui:field="_navbarPanel"/>
