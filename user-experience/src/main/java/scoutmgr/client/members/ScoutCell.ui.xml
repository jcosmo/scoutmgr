<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
  <ui:with field='firstName' type='com.google.gwt.safehtml.shared.SafeHtml'/>
  <ui:with field='lastName' type='com.google.gwt.safehtml.shared.SafeHtml'/>

  <g:HTMLPanel>
    <div>
      First Name:
      <ui:safehtml from="{firstName}"/>
      ,
      Last Name:
      <ui:safehtml from="{lastName}"/>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>