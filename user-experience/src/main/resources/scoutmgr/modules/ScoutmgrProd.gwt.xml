<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='scoutmgr'>
  <inherits name='scoutmgr.ScoutmgrApp'/>
  <!-- Provide gzipped version of generated js, so as it will sent compressed by jetty -->
  <inherits name="com.google.gwt.precompress.Precompress"/>
  <inherits name="com.google.gwt.logging.LoggingDisabled"/>

  <inherits name="org.realityforge.gwt.appcache.Appcache"/>
  <inherits name="org.realityforge.gwt.appcache.linker.Linker"/>

  <add-linker name="appcache"/>

  <!-- This reduces a lot the size of compiled javascript-->
  <set-property name="compiler.stackMode" value="strip"/>

  <extend-configuration-property name="appcache_static_files" value="img/loader.gif"/>
</module>
